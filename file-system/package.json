{
  "name": "file-system",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:file-system": "jest --config jest.config.file-system.ts",
    "test:file-system:watch": "jest --config jest.config.file-system.ts --watch",
    "test:file-system:coverage": "jest --config jest.config.file-system.ts --coverage",
    "test:file-system:step-1-1": "jest --config jest.config.file-system.ts --testPathPattern='(config|types|constants).*spec.ts$'",
    "test:file-system:step-1-2": "jest --config jest.config.file-system.ts --testPathPattern='garage.*spec.ts$'",
    "test:file-system:step-1-3": "jest --config jest.config.file-system.ts --testPathPattern='(file\\.entity|file-metadata\\.repository).*spec.ts$'",
    "test:file-system:step-2-1": "jest --config jest.config.file-system.ts --testPathPattern='(file-security\\.service|virus-scanner\\.service)\\.spec\\.ts$'",
    "test:file-system:step-2-2": "jest --config jest.config.file-system.ts --testPathPattern='(file-security\\.middleware|file-access\\.guard|file-audit\\.interceptor)\\.spec\\.ts$'",
    "test:file-system:phase-3-1": "jest --config jest.config.file-system.ts --testPathPattern='(image-processor|pdf-processor|document-processor|file-processing)\\.service\\.spec\\.ts$'",
    "dev:setup": "./setup-all.sh",
    "dev:start": "docker compose up -d",
    "dev:stop": "docker compose down",
    "dev:clean": "docker compose down -v",
    "dev:logs": "docker compose logs -f",
    "dev:status": "docker compose ps",
    "dev": "npm run dev:start && npm run start:dev",
    "db:migrate": "prisma migrate dev --schema=src/infrastructure/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=src/infrastructure/prisma/schema.prisma",
    "db:push": "prisma db push --schema=src/infrastructure/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=src/infrastructure/prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=src/infrastructure/prisma/schema.prisma",
    "garage:logs": "docker compose logs -f garage",
    "garage:shell": "docker exec -it garage-dev /bin/sh"
  },
  "dependencies": {
    "@bull-board/api": "^6.10.1",
    "@bull-board/express": "^6.10.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "axios": "^1.10.0",
    "bull": "^4.16.5",
    "cache-manager": "^7.0.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "pg": "^8.16.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.3",
    "@prisma/client": "^6.10.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bull": "^3.15.9",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.10.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
